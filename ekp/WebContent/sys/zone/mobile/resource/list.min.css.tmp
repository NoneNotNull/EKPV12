

.muiHeader{ margin-bottom:1px;}
/**list**/
.mui_zone_detail_show {
	display: none!important;
}

.muiZoneDetail {
	display: table;
	position: relative;
	padding: 10px 0px;
	width: 100%;
	background: #399be8;
	color: #ffffff; 
}
.muiZoneDetail .trig {
	display: block;
	width: 0px;
	height: 0px;
	border: 6px solid transparent;
	border-bottom-color: #399be8;
	position: absolute;
	top: -11px;
	left: 2.7rem;
}
.muiZoneDetail .mui_zone_detail_item_container {
	display:table-cell;
	width:20%;
	text-align: center; vertical-align:middle;
}
.muiZoneDetail .mui_zone_detail_item {
	display: inline-block;
	border: .1rem solid #fff;
	border-radius: 50%;
	height: 3.2rem;
	width: 3.2rem;
	line-height: 3.2rem;
	text-align: center;
	
}
.mui_zone_detail_item .mui {
	line-height:3.2rem;
	color:#fff;
	cursor: pointer; font-size:2.5rem;
}
.mui_zone_cate_icon {
	vertical-align: top!important;
}
.mui_zone_cate_icon img{
	height: 5rem!important;
	width: 5rem!important;
}
.mui_zone_cate_phone,.mui_zone_cate_post {
	color:#989898; font-size:1.3rem; line-height:1.6rem;
}
.mui_zone_cate_post { margin-bottom:.5rem;}
.mui_zone_cate_person_name {
	font-size: 1.7rem; font-weight:bold;
	color:#52B1F6;
	padding-top: .2rem;
	padding-bottom: .5rem;
}
.mui_zone_cate_phone .mui-tel,
.mui_zone_cate_post .mui-post{
	font-size: 2rem; margin-right:.3rem;
}
.mui_zone_cate_post .mui-post{ position:relative; font-size:2.2rem; margin-right:.5rem;  line-height:1.6rem;}

.muiCateInfo {
	padding-left: 1.2rem;
}

.mui_zone_content_border {
	border-bottom: .1rem solid #399be8;
}

.muiCateHeaderContent {
	min-height: 4rem;
}

.muiZoneContactUl {
	min-height: 10rem !important;
	margin-top: -25%;
	position: absolute;
	top: 50%;
	left: 50%;
	width: 90%;
	margin-left: -45%;
	-webkit-appearance: none;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	min-width: 20rem;
	min-height: 20rem;
	/* margin: 0;*/
	padding: 0;
}
.muiZoneContact {
	float: left;
	/* margin: 1.5rem 1.2rem; */
	width: 33%;
	text-align: center;
	margin:1rem 0;
	-webkit-transition: all 0.5s ease-out;
	-webkit-transform: translate3d(0px,1000px, 0px);
}
.muiZoneDetailItem {
	background: #399be8;
	display: inline-block;
	border-radius: 50%;
	height: 5.5rem;
	width: 5.5rem;
	line-height: 3.2rem;
	text-align: center;
	color: #fff;
	
}
.muiZoneDetailItemText {
	display: block;
	color: #fff;
	padding: 0.4rem 0;
}
.muiZoneDetailItem .mui {
	font-size: 4rem;
	line-height: 5.5rem;
	vertical-align: middle;
}
.muiZoneDetailItemBgColor5.{
	background-color: #399be8;
}
.muiZoneDetailItemBgColor0 {
	background-color: #E7507B;
}
.muiZoneDetailItemBgColor1 {
	background-color: #F26C60   
}
.muiZoneDetailItemBgColor2{
	background-color:#FDA106;
}
.muiZoneDetailItemBgColor3{
	background-color:#34B87F;
}
.muiZoneDetailItemBgColor4{
	background-color:#66CDCC;
}
.muiZoneContactMask {
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	background-color: rgba(0, 0, 0, 0.7);
	z-index: 2;
	overflow: hidden;
	-webkit-transition: opacity 0.2s linear;
	opacity: 0;
}
/*关注按钮样式*/
.muiCateMore {
    
	text-align: left;
	vertical-align: top!important;
	cursor: pointer;
}
.mui_zone_follow_icon { margin-left:0.5rem; margin-top:0.5rem;
	font-size: 3rem!important;
	line-height: 3.2rem;
}



/*** 地址本 ***/
.muiGroupItem{ background:#f4f4f4; border-bottom:1px solid #dfdfdf;}
.muiCateTitle{ padding:.3rem .5rem;}



.muiSearchInput{ -webkit-appearance: none; -webkit-tap-highlight-color:rgba(255,255,255,0); padding-top:0.4em; height:2.3rem; line-height:2.3rem;}
.muiCateHeaderReturn .mui-back{ font-size:1.8rem;}
.muiCateHeaderReturn{ -webkit-tap-highlight-color:rgba(255,255,255,0); }

.muiCateMore .mui-forward{ margin-left:2rem;}
.muiCateNav{ text-align:center; right:0.1rem;}
.muiCateNav .muiCateNavItem{ font-size:10px!important; }
.muiCateNav .muiCateNavItem .mui-search{  font-weight:bold;}









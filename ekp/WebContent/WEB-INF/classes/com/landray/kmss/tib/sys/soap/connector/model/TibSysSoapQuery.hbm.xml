<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.tib.sys.soap.connector.model.TibSysSoapQuery"
		table="tib_sys_soap_query">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docSubject"
			column="doc_subject"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docInputParam"
			column="doc_input_param"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

		<property
			name="docOutputParam"
			column="doc_output_param"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />
			
		<property
			name="docFaultInfo"
			column="doc_fault_info"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="tibSysSoapMain"
			column="tib_sys_soap_main"
			insert="true"
			update="true"
			not-null="false" />

	</class>
</hibernate-mapping>

			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdMarks"
			column="fd_marks"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdWsdlVersion"
			column="fd_wsdl_version"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator"
			insert="true"
			update="true"
			not-null="false" />
			
				
		 <many-to-one 
		        name="settCategory" 
		        column="sett_category_id"
				insert="true"
				update="true" 
				not-null="false" />
	

		<bag
			name="fdServerExt"
			cascade="all-delete-orphan"
			inverse="true"
			lazy="true">
			<key
				column="fd_server_setting" />
			<one-to-many
				class="com.landray.kmss.tib.sys.soap.connector.model.TibSysSoapSettingExt" />
		</bag>

	</class>
</hibernate-mapping>

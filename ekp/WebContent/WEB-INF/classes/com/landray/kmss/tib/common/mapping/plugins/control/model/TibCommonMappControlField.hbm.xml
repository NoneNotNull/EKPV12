<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.tib.common.mapping.plugins.control.model.TibCommonMappControlField"
		table="tib_common_mapp_control_field">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="uuId"
			column="uu_id"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="conditionsUuid"
			column="conditions_uuid"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fieldValue"
			column="field_value"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="rowIndex"
			column="row_index"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

	</class>
</hibernate-mapping>

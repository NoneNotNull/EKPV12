<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.example.rules.model.ExampleRulesMain"
		table="example_rules_main">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docSubject"
			column="doc_subject"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdWorkType"
			column="fd_work_type"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

		<property
			name="docPublishTime"
			column="doc_publish_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docContent"
			column="doc_content"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

		<property
			name="fdNotifyType"
			column="fd_notify_type"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="true" />

		<many-to-one
			name="docCategory"
			column="doc_category_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<bag
			name="fdNotifiers"
			table="sys_org_notifiers"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>

		<bag
			name="authReaders"
			table="example_rules_main_reader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>

		<bag
			name="authEditors"
			table="example_rules_main_editor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>

		<bag
			name="authOtherReaders"
			table="example_rules_main_oreader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>

		<bag
			name="authAllReaders"
			table="example_rules_main_areader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>

		<bag
			name="authOtherEditors"
			table="example_rules_main_oeditor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>

		<bag
			name="authAllEditors"
			table="example_rules_main_aeditor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>


	</class>
</hibernate-mapping>

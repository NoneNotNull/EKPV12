<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.kms.knowledge.model.KmsKnowledgeCategory"
		table="kms_knowledge_category">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		
		<property
			name="fdTemplateType"
			column="fd_template_type"
			update="true"
			insert="true"
			length="2"
			not-null="false" />
		
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="true" />		
		<many-to-one
			name="docAlteror"
			column="doc_alteror_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" />


		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />


		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="hbmParent"
			class="com.landray.kmss.kms.knowledge.model.KmsKnowledgeCategory"
			cascade="save-update"
			outer-join="auto"
			update="true"
			insert="true"
			access="property"
			column="fd_parent_id" />

		<property
			name="fdHierarchyId"
			column="fd_hierarchy_id"
			update="true"
			insert="true"
			length="300"
			not-null="false" />

		<property
			name="fdDescription"
			column="fd_description"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />
			
		<property
			name="docExpire"
			column="doc_expire"
			update="true"
			insert="true"
			not-null="false"
			length="10" />
			
		<property
			name="fdIsinheritMaintainer"
			column="fd_isinherit_maintainer"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdIsinheritUser"
			column="fd_isinherit_user"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="authReaderFlag"
			column="auth_reader_flag"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="authNotReaderFlag"
			column="auth_not_reader_flag"
			update="true"
			insert="true"
			length="36"
			not-null="false" />


		<property
			name="fdNumberPrefix"
			column="fd_number_prefix"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<property
			name="authTmpAttNodownload"
			column="auth_tmp_att_nodownload"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="authTmpAttNocopy"
			column="auth_tmp_att_nocopy"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="authTmpAttNoprint"
			column="auth_tmp_att_noprint"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<bag
			name="authTmpReaders"
			table="kms_knowledge_cate_m_reader"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		<bag
			name="authTmpEditors"
			table="kms_knowledge_cate_m_editor"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		<bag
			name="authReaders"
			table="kms_knowledge_cate_reader"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		<bag
			name="authEditors"
			table="kms_knowledge_cate_editor"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		

		<bag
			name="authTmpAttPrints"
			table="kms_knowledge_cate_m_attprint"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_print_id" />
		</bag>
		<bag
			name="authTmpAttCopys"
			table="kms_knowledge_cate_m_attcopy"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_copy_id" />
		</bag>
		<bag
			name="authTmpAttDownloads"
			table="kms_knowledge_cate_m_attdl"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_download_id" />
		</bag>
		<bag
			name="authAllEditors"
			table="kms_knowledge_cate_aeditor"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		
		<bag
			name="authAllReaders"
			table="kms_knowledge_cate_areader"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		<bag
			name="authOtherReaders"
			table="kms_knowledge_cate_oreader"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		<bag
			name="authOtherEditors"
			table="kms_knowledge_cate_oeditor"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>
		
		<bag
			name="docPosts"
			table="kms_knowledge_category_post"
			lazy="true">
			<key column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_post_id" />
		</bag>
		
		<!-- 场所 -->
		<many-to-one
			name="authArea"
			column="auth_area_id"
			insert="true"
			update="true"
			not-null="false"
			index="idx_kmsKnowledgeCategory_authAreaId" />
			
		<many-to-one
			name="sysPropertyTemplate"
			column="fd_prop_template_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="wikiTemplate"
			column="wiki_template_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<many-to-one
			name="docTemplate"
			column="doc_template_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<bag
			name="knowledges"
			table="kms_knowledge_secondcategory"
			lazy="true">
			<key
				column="fd_second_category_id" />
			<many-to-many
				class="com.landray.kmss.kms.knowledge.model.KmsKnowledgeBaseDoc"
				column="fd_doc_id" />
		</bag>

	</class>
</hibernate-mapping>

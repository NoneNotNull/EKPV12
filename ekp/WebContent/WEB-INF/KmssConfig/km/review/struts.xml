<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<!-- 表单 Bean -->
	<form-beans>
		<form-bean
			name="kmReviewMainForm"
			type="com.landray.kmss.km.review.forms.KmReviewMainForm">
		</form-bean>
		<form-bean
			name="kmReviewFeedbackInfoForm"
			type="com.landray.kmss.km.review.forms.KmReviewFeedbackInfoForm">
		</form-bean>
		<form-bean
			name="kmReviewTemplateForm"
			type="com.landray.kmss.km.review.forms.KmReviewTemplateForm">
		</form-bean>
		<form-bean
			name="kmReviewTemplateKeywordForm"
			type="com.landray.kmss.km.review.forms.KmReviewTemplateKeywordForm">
		</form-bean>
		<form-bean
			name="kmReviewDocKeywordForm"
			type="com.landray.kmss.km.review.forms.KmReviewDocKeywordForm">
		</form-bean>
		<form-bean
			name="kmReviewMainSearchForm"
			type="com.landray.kmss.km.review.forms.KmReviewMainSearchForm">
		</form-bean>
		<form-bean
			name="kmReviewCommonConfigForm"
			type="com.landray.kmss.km.review.forms.KmReviewCommonConfigForm">
		</form-bean>
	</form-beans>

	<!-- 操作映射 -->
	<action-mappings> 
		<action
			path="/km/review/km_review_main/kmReviewMain"
			type="com.landray.kmss.km.review.actions.KmReviewMainAction"
			name="kmReviewMainForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="view"
				path="/km/review/km_review_ui/kmReviewMain_view.jsp">
			</forward>
			<forward
				name="view.old"
				path="/km/review/km_review_main/kmReviewMain_view.jsp">
			</forward>
			<forward
				name="edit"
				path="/km/review/km_review_ui/kmReviewMain_edit.jsp">
			</forward>
			<forward
				name="edit.old"
				path="/km/review/km_review_main/kmReviewMain_edit.jsp">
			</forward>
			<forward
				name="edit.4pda"
				path="/km/review/km_review_main/kmReviewMain_edit_pda.jsp">
			</forward>
			<forward
				name="list.4m"
				path="/km/review/mobile/list.jsp">
			</forward>
			<forward
				name="listChildren.4m"
				path="/km/review/mobile/list.jsp">
			</forward>
			<forward
				name="list"
				path="/km/review/km_review_main/kmReviewMain_list.jsp">
			</forward>
			<forward
				name="changeTemp"
				path="/km/review/km_review_main/kmReveiwChangeTemplate.jsp">
			</forward>
			<forward
				name="print"
				path="/km/review/km_review_main/kmReviewMain_print.jsp">
			</forward>
			<forward
				name="searchResult"
				path="/km/review/km_review_main/kmReviewMain_result.jsp">
			</forward>
			<forward
				name="addRealtionDoc"
				path="/km/review/km_review_main/kmReviewMainRelaDoc.jsp">
			</forward>
			<forward
				name="listChildren"
				path="/km/review/km_review_main/kmReviewMain_list.jsp">
			</forward>
			<forward
				name="editRight"
				path="/km/review/km_review_main/kmReviewMain_right.jsp">
			</forward>
			<forward
				name="manageList"
				path="/km/review/km_review_main/kmReviewMain_list.jsp">
			</forward>
			<forward
				name="previewReview"
				path="/km/review/km_review_main/kmReviewMain_preview.jsp">
			</forward>
			<forward
				name="view.4m"
				path="/km/review/mobile/view.jsp">
			</forward>
			<forward
				name="edit.4m"
				path="/km/review/mobile/edit.jsp">
			</forward>
		</action>


		<action
			path="/km/review/km_review_feedback_info/kmReviewFeedbackInfo"
			type="com.landray.kmss.km.review.actions.KmReviewFeedbackInfoAction"
			name="kmReviewFeedbackInfoForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="view"
				path="/km/review/km_review_feedback_info/kmReviewFeedbackInfo_view.jsp">
			</forward>
			<forward
				name="edit"
				path="/km/review/km_review_feedback_info/kmReviewFeedbackInfo_edit.jsp">
			</forward>
			<forward
				name="list"
				path="/km/review/km_review_feedback_info/kmReviewFeedbackInfo_list.jsp">
			</forward>
			<forward
				name="listdata"
				path="/km/review/km_review_feedback_info/kmReviewFeedbackInfo_listdata.jsp">
			</forward>
		</action>

		<action
			path="/km/review/km_review_template/kmReviewTemplate"
			type="com.landray.kmss.km.review.actions.KmReviewTemplateAction"
			name="kmReviewTemplateForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="view"
				path="/km/review/km_review_template/kmReviewTemplate_view.jsp">
			</forward>
			<forward
				name="edit"
				path="/km/review/km_review_template/kmReviewTemplate_edit.jsp">
			</forward>
			<forward
				name="list"
				path="/km/review/km_review_template/kmReviewTemplate_list.jsp">
			</forward>
			<forward
				name="listChildren"
				path="/km/review/km_review_template/kmReviewTemplate_list.jsp">
			</forward>
		</action>
		<action
			path="/km/review/km_review_common_config/kmReviewCommonConfig"
			type="com.landray.kmss.km.review.actions.KmReviewCommonConfigAction"
			name="kmReviewCommonConfigForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="edit"
				path="/km/review/km_review_common_config/kmReviewCommonConfig_edit.jsp">
			</forward>
			<forward
				name="list"
				path="/km/review/km_review_common_config/kmReviewCommonConfig_list.jsp">
			</forward>
		</action>
		
		<action
			path="/km/review/km_review_index/kmReviewIndex"
			type="com.landray.kmss.km.review.actions.KmReviewIndexAction"
			name="kmReviewMainForm"
			parameter="method"
			input="edit">
			<forward
				name="list"
				path="/km/review/km_review_ui/kmReviewMain_list.jsp">
			</forward>
			<forward
				name="list.4m"
				path="/km/review/mobile/list.jsp">
			</forward>
		</action>
		<action
			path="/km/review/km_review_main/kmReviewMainPortlet"
			type="com.landray.kmss.km.review.actions.KmReviewMainPortletAction"
			parameter="method"
			input="edit">
			<forward
				name="listPortlet"
				path="/km/review/km_review_ui/kmReviewMainPortlet_list.jsp">
			</forward>
		</action>
	</action-mappings>

	<!-- 消息资源 -->
	<message-resources
		parameter="com.landray.kmss.km.review.ApplicationResources"
		key="km-review" />


</struts-config>

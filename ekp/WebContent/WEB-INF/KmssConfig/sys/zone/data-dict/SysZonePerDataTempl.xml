<?xml version="1.0" encoding="UTF-8"?>
<models
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
	<model
		modelName="com.landray.kmss.sys.zone.model.SysZonePerDataTempl"
		messageKey="sys-zone:table.sysZonePerDataTempl"
		displayProperty="fdName"
		serviceBean="sysZonePerDataTemplService"
		table="sys_zone_per_data_templ"
		url="/sys/zone/sys_zone_per_data_templ/sysZonePerDataTempl.do?method=view&amp;fdId=${fdId}">

		<idProperty>
			<generator
				type="assigned" />
		</idProperty>

		<simpleProperty
			name="fdName"
			messageKey="sys-zone:sysZonePerDataTempl.fdName"
			type="String"
			column="fd_name"
			notNull="true"
			length="200"
			validate="true" />

		<simpleProperty
			name="docContent"
			messageKey="sys-zone:sysZonePerDataTempl.docContent"
			type="RTF"
			column="doc_content"
			notNull="false"
			length="1000000"
			validate="false" />

		<simpleProperty
			name="fdOrder"
			messageKey="sys-zone:sysZonePerDataTempl.fdOrder"
			type="Integer"
			column="fd_order"
			notNull="false"
			length="4"
			validate="false" />

 

 
	</model>
</models>

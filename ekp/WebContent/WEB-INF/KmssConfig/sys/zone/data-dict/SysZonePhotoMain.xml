<?xml version="1.0" encoding="UTF-8"?>
<models
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
	<model
		modelName="com.landray.kmss.sys.zone.model.SysZonePhotoMain"
		messageKey="sys-zone:table.sysZonePhotoMain"
		displayProperty="fdName"
		serviceBean="sysZonePhotoMainService"
		table="sys_zone_photo_main"
		url="/sys/zone/sys_zone_photo_main/sysZonePhotoMain.do?method=view&amp;fdId=${fdId}">

		<idProperty>
			<generator
				type="assigned" />
		</idProperty>

		<simpleProperty
			name="fdName"
			messageKey="sys-zone:sysZonePhotoMain.fdName"
			type="String"
			column="fd_name"
			notNull="false"
			length="200"
			validate="true" />

		<simpleProperty
			name="docCreateTime"
			messageKey="sys-zone:sysZonePhotoMain.docCreateTime"
			type="DateTime"
			column="doc_create_time"
			notNull="false"
			validate="false" />

		<simpleProperty
			name="fdLastModifiedTime"
			messageKey="sys-zone:sysZonePhotoMain.fdLastModifiedTime"
			type="DateTime"
			column="fd_last_modified_time"
			notNull="false"
			validate="false" />

		<simpleProperty
			name="fdIsDefault"
			messageKey="sys-zone:sysZonePhotoMain.fdIsDefault"
			type="String"
			column="fd_is_default"
			notNull="false"
			length="1"
			validate="false" />

		<simpleProperty
			name="fdTemplateId"
			messageKey="sys-zone:sysZonePhotoMain.fdTemplateId"
			type="String"
			column="fd_template_id"
			notNull="false"
			length="36"
			validate="false" />

		<simpleProperty
			name="fdTemplateName"
			messageKey="sys-zone:sysZonePhotoMain.fdTemplateName"
			type="String"
			column="fd_template_name"
			notNull="false"
			length="100"
			validate="false" />
		<simpleProperty
			name="fdSourceId"
			messageKey=""
			type="String"
			column="fd_template_id"
			notNull="false"
			length="36"
			validate="false" />
		<simpleProperty
			name="fdSourceName"
			messageKey=""
			type="String"
			column="fd_source_name"
			notNull="false"
			length="100"
			validate="false" />	
	</model>
</models>

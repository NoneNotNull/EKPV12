<?xml version="1.0" encoding="UTF-8"?>
<models
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
	<model
		modelName="com.landray.kmss.sys.lbpm.engine.persistence.model.LbpmErrorQueue"
		messageKey="sys-lbpm-engine:table.lbpmErrorQueue"
		serviceBean="lbpmErrorQueueService"
		table="lbpm_error_queue">

		<idProperty>
			<generator
				type="assigned" />
		</idProperty>

		<simpleProperty
			name="fdProcessId"
			messageKey="sys-lbpm-engine:lbpmErrorQueue.fdProcessId"
			type="String"
			column="fd_process_id"
			notNull="false"
			length="36" />

		<simpleProperty
			name="fdRootProcessId"
			messageKey="sys-lbpm-engine:lbpmErrorQueue.fdRootProcessId"
			type="String"
			column="fd_root_process_id"
			notNull="false"
			length="36" />

		<simpleProperty
			name="fdTaskId"
			messageKey="sys-lbpm-engine:lbpmErrorQueue.fdTaskId"
			type="String"
			column="fd_task_id"
			notNull="false"
			length="36" />

		<simpleProperty
			name="fdActivityType"
			messageKey="sys-lbpm-engine:lbpmErrorQueue.fdActivityType"
			type="String"
			column="fd_activity_type"
			notNull="false"
			length="100" />

		<simpleProperty
			name="fdUserId"
			messageKey="sys-lbpm-engine:lbpmErrorQueue.fdUserId"
			type="String"
			column="fd_user_id"
			notNull="false"
			length="36" />

		<simpleProperty
			name="fdParameter"
			messageKey="sys-lbpm-engine:lbpmErrorQueue.fdParameter"
			type="RTF"
			column="fd_parameter"
			notNull="false"
			length="1000000" />

		<simpleProperty
			name="fdPriority"
			messageKey="sys-lbpm-engine:lbpmErrorQueue.fdPriority"
			type="Integer"
			column="fd_priority"
			notNull="false"
			length="10" />

		<simpleProperty
			name="fdErrorNodeFactId"
			messageKey="sys-lbpm-engine:lbpmErrorQueue.fdErrorNodeFactId"
			type="String"
			column="fd_error_node_fact_id"
			notNull="false"
			length="50" />

		<simpleProperty
			name="fdSourceNodeFactId"
			messageKey="sys-lbpm-engine:lbpmErrorQueue.fdSourceNodeFactId"
			type="String"
			column="fd_source_node_fact_id"
			notNull="false"
			length="50" />

		<simpleProperty
			name="fdErrorType"
			messageKey="sys-lbpm-engine:lbpmErrorQueue.fdErrorType"
			type="String"
			column="fd_error_type"
			notNull="false"
			length="50" />

		<simpleProperty
			name="fdErrorMessage"
			messageKey="sys-lbpm-engine:lbpmErrorQueue.fdErrorMessage"
			type="String"
			column="fd_error_message"
			notNull="false"
			length="1500" />

	</model>
</models>
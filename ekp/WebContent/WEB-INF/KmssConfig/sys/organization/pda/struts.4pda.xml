<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<action-mappings>
		<action path="/sys/organization/pda/address"
			type="com.landray.kmss.sys.organization.pda.PdaAddressAction" parameter="method"
			unknown="false" validate="false" scope="request">
			<forward name="address" path="/sys/organization/pda/address.jsp" />
			<forward name="search" path="/sys/organization/pda/search.jsp" />
		</action>
	</action-mappings>

	<message-resources
		parameter="com.landray.kmss.sys.organization.pda.ApplicationResources"
		key="sys-organization-pda"/>
</struts-config>

<?xml version="1.0" encoding="UTF-8"?>
<models
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
	<model
		modelName="com.landray.kmss.sys.organization.model.SysOrgPerson"
		messageKey="sys-organization:sysOrgElement.person"
		displayProperty="fdName"
		serviceBean="sysOrgPersonService"
		table="sys_org_person"
		extendClass="com.landray.kmss.sys.organization.model.SysOrgElement"
		discriminatorValue="8"
		url="/sys/organization/sys_org_person/sysOrgPerson.do?method=view&amp;fdId=${fdId}">
		<simpleProperty
			name="fdName"
			messageKey="sys-organization:sysOrgPerson.fdName"
			type="String"
			column="fd_name"
			notNull="true"
			unique="false"
			length="100"
			canSearch="true" 
			validate="true"/>
		<simpleProperty
			name="fdOrder"
			messageKey="sys-organization:sysOrgPerson.fdOrder"
			type="Integer"
			column="fd_order"
			notNull="false"
			unique="false" 
			validate="true"/>
		<simpleProperty
			name="fdNo"
			messageKey="sys-organization:sysOrgPerson.fdNo"
			type="String"
			column="fd_no"
			notNull="false"
			unique="false"
			length="100"
			canSearch="true" 
			validate="true"/>
		<simpleProperty
			name="fdSex"
			messageKey="sys-organization:sysOrgPerson.fdSex"
			type="String"
			column="fd_sex"
			notNull="false"
			unique="false"
			length="50"
			canSearch="true" />
		<simpleProperty
			name="fdKeyword"
			messageKey="sys-organization:sysOrgPerson.fdKeyword"
			type="String"
			column="fd_keyword"
			notNull="false"
			unique="false"
			length="100"
			canSearch="true" 
			validate="true"/>
		<simpleProperty
			name="fdIsAvailable"
			messageKey="sys-organization:sysOrgPerson.fdIsAvailable"
			type="Boolean"
			column="fd_is_available"
			notNull="false"
			unique="false"
			defaultValue="1"
			canSearch="true" />
		<simpleProperty
			name="fdIsBusiness"
			messageKey="sys-organization:sysOrgPerson.fdIsBusiness"
			type="Boolean"
			column="fd_is_business"
			notNull="false"
			unique="false"
			defaultValue="1"
			canSearch="true" />
		<simpleProperty
			name="fdMemo"
			messageKey="sys-organization:sysOrgPerson.fdMemo"
			type="String"
			column="fd_memo"
			notNull="false"
			unique="false"
			length="1500"
			canSearch="true" 
			validate="true"/>
		<modelProperty
			name="hbmParent"
			messageKey="sys-organization:sysOrgPerson.fdParent"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			column="fd_parentid"
			notNull="false"
			unique="false"
			canSearch="true"
			treeModel="true"
			dialogJS="Dialog_Address(!{mulSelect}, '!{idField}', '!{nameField}', null, ORG_TYPE_ORGORDEPT);"
			where="sysOrgElement.fdOrgType=1 or sysOrgElement.fdOrgType=2" />
		<listProperty
			name="hbmPosts"
			messageKey="sys-organization:sysOrgPerson.fdPosts"
			column="fd_personid"
			type="com.landray.kmss.sys.organization.model.SysOrgPost"
			table="sys_org_post_person"
			elementColumn="fd_postid"
			inverse="false"
			dialogJS="Dialog_Address(true, '!{idField}', '!{nameField}', null, ORG_TYPE_POST);" />
		
		<simpleProperty
			name="fdMobileNo"
			messageKey="sys-organization:sysOrgPerson.fdMobileNo"
			type="String"
			column="fd_mobile_no"
			notNull="false"
			unique="false"
			length="100"
			canSearch="true" 
			validate="true"/>
		<simpleProperty
			name="fdWorkPhone"
			messageKey="sys-organization:sysOrgPerson.fdWorkPhone"
			type="String"
			column="fd_work_phone"
			notNull="false"
			unique="false"
			length="100"
			canSearch="true" 
			validate="true"/>
		<simpleProperty
			name="fdEmail"
			messageKey="sys-organization:sysOrgPerson.fdEmail"
			type="String"
			column="fd_email"
			notNull="false"
			unique="false"
			length="200"
			canSearch="true"
			validate="true" />
		<simpleProperty
			name="fdLoginName"
			messageKey="sys-organization:sysOrgPerson.fdLoginName"
			type="String"
			column="fd_login_name"
			notNull="true"
			unique="false"
			length="200"
			canSearch="true" 
			validate="true"/>
		<simpleProperty
			name="fdPassword"
			messageKey="sys-organization:sysOrgPerson.fdPassword"
			type="String"
			column="fd_password"
			notNull="false"
			unique="false"
			length="254"
			canDisplay="true" />
		<simpleProperty
			name="fdNewPassword"
			messageKey="sys-organization:sysOrgPerson.newPassword"
			type="String"
			column="fd_new_password"
			notNull="false"
			unique="false"
			length="254"
			canDisplay="false" />
		<simpleProperty
			name="fdInitPassword"
			messageKey="sys-organization:sysOrgPerson.fdInitPassword"
			type="String"
			column="fd_init_password"
			notNull="false"
			unique="false"
			length="254"
			canDisplay="false" />
		<simpleProperty
			name="fdRtxNo"
			messageKey="sys-organization:sysOrgPerson.fdRtxNo"
			type="String"
			column="fd_rtx_no"
			notNull="false"
			unique="false"
			length="100"
			canSearch="true" 
			validate="true"/>
		<simpleProperty
			name="fdWechatNo"
			messageKey="sys-organization:sysOrgPerson.fdWechatNo"
			type="String"
			column="fd_wechat_no"
			notNull="false"
			unique="false"
			length="100"
			canSearch="true" 
			validate="true"/>
		<simpleProperty
			name="fdCardNo"
			messageKey="sys-organization:sysOrgPerson.fdCardNo"
			type="String"
			column="fd_card_no"
			notNull="false"
			unique="false"
			length="100"
			canSearch="true" />
		<simpleProperty
			name="fdDefaultLang"
			messageKey="sys-organization:sysOrgPerson.fdDefaultLang"
			type="String"
			column="fd_default_lang"
			notNull="false"
			unique="false"
			length="50"
			canSearch="true" />
	</model>
</models>
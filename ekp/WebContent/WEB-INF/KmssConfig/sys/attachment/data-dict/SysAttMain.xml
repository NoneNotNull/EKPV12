<?xml version="1.0" encoding="UTF-8"?>
<models
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
	<model
		modelName="com.landray.kmss.sys.attachment.model.SysAttMain"
		messageKey="sys-attachment:table.sysAttMain"
		serviceBean="sysAttMainService"
		table="sys_att_main"
		url="/sys/attachment/sys_att_main/sysAttMain.do?method=download&amp;fdId=${fdId}">
		<idProperty>
			<generator
				type="assigned" />
		</idProperty>
		<simpleProperty
			name="fdData"
			messageKey="sys-attachment:sysAttMain.fdData"
			type="RTF"
			length="1000000"
			column="fd_data"
			unique="false" />
		<simpleProperty
			name="fdModelId"
			messageKey="sys-attachment:sysAttMain.fdModelId"
			type="String"
			column="fd_model_id"
			notNull="false"
			unique="false"
			length="36" />
		<simpleProperty
			name="fdModelName"
			messageKey="sys-attachment:sysAttMain.fdModelName"
			type="String"
			column="fd_model_name"
			notNull="false"
			unique="false"
			length="500" />
		<simpleProperty
			name="fdKey"
			messageKey="sys-attachment:sysAttMain.fdKey"
			type="String"
			column="fd_key"
			notNull="false"
			unique="false"
			length="500" />
		<simpleProperty
			name="fdFileName"
			messageKey="sys-attachment:sysAttMain.fdFileName"
			type="String"
			column="fd_file_name"
			notNull="false"
			unique="false"
			length="2000" />
		<simpleProperty
			name="fdContentType"
			messageKey="sys-attachment:sysAttMain.fdContentType"
			type="String"
			column="fd_content_type"
			notNull="false"
			unique="false"
			length="200" />
		<simpleProperty
			name="fdAttType"
			messageKey="sys-attachment:sysAttMain.fdAttType"
			type="String"
			column="fd_att_type"
			notNull="false"
			unique="false"
			length="20" />
		<simpleProperty
			name="docCreateTime"
			messageKey="sys-attachment:sysAttMain.docCreateTime"
			type="DateTime"
			column="doc_Create_Time"
			notNull="false"
			unique="false" />
		<simpleProperty
			name="fdFilePath"
			messageKey="sys-attachment:sysAttMain.fdFilePath"
			type="String"
			column="fd_file_path"
			notNull="false"
			unique="false"
			length="4000" />
		<simpleProperty
			name="fdSize"
			messageKey="sys-attachment:sysAttMain.fdSize"
			type="Double"
			column="fd_size"
			notNull="false"
			unique="false"
			length="32" />
		<simpleProperty
			name="fdAttLocation"
			messageKey="sys-attachment:sysAttMain.fdAttLocation"
			type="String"
			column="fd_att_location"
			notNull="false"
			unique="false"
			length="32" />
		<simpleProperty
			name="fdCreatorId"
			messageKey="sys-attachment:sysAttMain.fdCreatorId"
			type="String"
			column="fd_creator_id"
			notNull="false"
			unique="false"
			length="32" />
		<simpleProperty
			name="downloadSum"
			messageKey="sys-attachment:sysAttMain.downloadSum"
			type="Integer"
			column="download_sum"
			notNull="false"
			unique="false"
			length="8" />
		<simpleProperty
			name="fdOrder"
			messageKey="sys-attachment:sysAttMain.fdOrder"
			type="Integer"
			column="fd_order"
			notNull="false"
			unique="false"
			length="4" />
		<simpleProperty
			name="fdFileId"
			messageKey="sys-attachment:sysAttMain.fdFileId"
			type="String"
			column="fd_file_id"
			notNull="false"
			unique="false"
			length="36" />
	</model>
</models>
<?xml version="1.0" encoding="UTF-8"?>
<models
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
	<model
		modelName="com.landray.kmss.sys.ftsearch.expand.model.SysFtsearchReadLog"
		messageKey="sys-ftsearch-expand:table.sysFtsearchReadLog"
		displayProperty="fdDocSubject"
		serviceBean="sysFtsearchReadLogService"
		table="sys_ftsearch_read_log"
		url="/sys/ftsearch/expand/sys_ftsearch_read_log/sysFtsearchReadLog.do?method=view&amp;fdId=${fdId}">

		<idProperty>
			<generator
				type="assigned" />
		</idProperty>

		<simpleProperty
			name="fdDocSubject"
			messageKey="sys-ftsearch-expand:sysFtsearchReadLog.fdDocSubject"
			type="String"
			column="fd_doc_subject"
			notNull="true"
			length="200"
			validate="true" />

		<simpleProperty
			name="fdUrl"
			messageKey="sys-ftsearch-expand:sysFtsearchReadLog.fdUrl"
			type="String"
			column="fd_url"
			notNull="false"
			length="1000"
			validate="true" />

		<simpleProperty
			name="fdModelId"
			messageKey="sys-ftsearch-expand:sysFtsearchReadLog.fdModelId"
			type="String"
			column="fd_model_id"
			notNull="false"
			length="36"
			validate="true" />

		<simpleProperty
			name="fdModelName"
			messageKey="sys-ftsearch-expand:sysFtsearchReadLog.fdModelName"
			type="String"
			column="fd_model_name"
			notNull="false"
			length="200"
			validate="true" />

		<simpleProperty
			name="fdCategoryHierarchyId"
			messageKey="sys-ftsearch-expand:sysFtsearchReadLog.fdCategoryHierarchyId"
			type="String"
			column="fd_category_hierarchy_id"
			notNull="false"
			length="1000"
			validate="true" />
	
		<simpleProperty
			name="fdReaderId"
			messageKey="sys-ftsearch-expand:sysFtsearchReadLog.fdReaderId"
			type="String"
			column="fd_reader_id"
			notNull="false"
			length="36"
			validate="true" />
			
		<simpleProperty
			name="fdReaderName"
			messageKey="sys-ftsearch-expand:sysFtsearchReadLog.fdReaderName"
			type="String"
			column="fd_reader_name"
			notNull="false"
			length="200"
			validate="true" />

		<simpleProperty
			name="fdReadTime"
			messageKey="sys-ftsearch-expand:sysFtsearchReadLog.fdReadTime"
			type="DateTime"
			column="fd_read_time"
			notNull="false"
			validate="true" />

		<simpleProperty
			name="fdSearchWord"
			messageKey="sys-ftsearch-expand:sysFtsearchReadLog.fdSearchWord"
			type="String"
			column="fd_search_word"
			notNull="false"
			length="255"
			validate="true" />

		<simpleProperty
			name="fdHitPosition"
			messageKey="sys-ftsearch-expand:sysFtsearchReadLog.fdHitPosition"
			type="Integer"
			column="fd_hit_position"
			notNull="false"
			length="10"
			validate="true" />

	</model>
</models>

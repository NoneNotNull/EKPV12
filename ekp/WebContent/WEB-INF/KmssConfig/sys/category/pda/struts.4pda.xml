<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<!-- 操作映射 -->
	<action-mappings>
		<action path="/sys/category/pda/sysCategory"
			type="com.landray.kmss.sys.category.pda.PdaCategroyAction" parameter="method"
			unknown="false" validate="false" scope="request">
			<forward name="listCate" path="/sys/category/pda/sysCategory.jsp">
			</forward>
			<forward name="pdaCategory" path="/sys/category/pda/sysCategory2.jsp">
			</forward>
		</action>
	</action-mappings>
	<message-resources
		parameter="com.landray.kmss.sys.category.pda.ApplicationResources"
		key="sys-category-pda"/>
</struts-config>

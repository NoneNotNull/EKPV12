<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<!-- 表单 Bean -->
	<form-beans>
		<form-bean
			name="sysRelationMainForm"
			type="com.landray.kmss.sys.relation.forms.SysRelationMainForm" />
		<form-bean
			name="sysRelationForeignModuleForm"
			type="com.landray.kmss.sys.relation.forms.SysRelationForeignModuleForm" />
		<form-bean
			name="sysRelationForeignParamForm"
			type="com.landray.kmss.sys.relation.forms.SysRelationForeignParamForm" />
		<form-bean
			name="sysRelationFileUploadForm"
			type="com.landray.kmss.sys.relation.forms.SysRelationFileUploadForm" />
		<form-bean
			name="sysRelationStaticNewForm"
			type="com.landray.kmss.sys.relation.forms.SysRelationStaticNewForm" />
	</form-beans>
	<!-- 操作映射 -->
	<action-mappings>
		<action
			path="/sys/relation/sys_relation_main/sysRelationMain"
			type="com.landray.kmss.sys.relation.actions.SysRelationMainAction"
			name="sysRelationMainForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="edit"
				path="/sys/relation/sys_relation_main/sysRelationMain_edit.jsp">
			</forward>
			<forward
				name="view"
				path="/sys/relation/sys_relation_main/sysRelationMain_view.jsp">
			</forward>
			<forward
				name="preview"
				path="/sys/relation/include/sysRelationMain_preview.jsp">
			</forward>
			<forward 
				name="listUi" 
				path="/sys/relation/import/sysRelationMain_list.jsp">
			</forward>
			<forward
				name="relationResults"
				path="/sys/relation/sys_relation_main/sysRelationMain_url.jsp">
			</forward>
			<forward
				name="docView"
				path="/sys/relation/sys_relation_main/sysRelationMain_doc_view.jsp">
			</forward>
			<forward
				name="docView.4pda"
				path="/sys/relation/sys_relation_main/sysRelationMain_doc_pda.jsp">
			</forward>
		</action>
		<!-- 全文检索的Action-->
		<action
			path="/sys/relation/otherurl/ftsearch/searchBuilder"
			type="com.landray.kmss.sys.relation.actions.SysRelationSearchAction"
			parameter="method"
			name="sysFtearchBuilderForm"
			scope="request">
			<forward
				name="searchPage"
				path="/sys/relation/sys_relation_search/search.jsp">
			</forward>
			<forward
				name="advancedSearchPage"
				path="/sys/relation/sys_relation_search/advanced_search.jsp">
			</forward>
			<forward
				name="result"
				path="/sys/relation/sys_relation_search/result.jsp">
			</forward>
			<forward
				name="resultUi"
				path="/sys/relation/import/static/sysRelationMain_search_result.jsp">
			</forward>
			<forward
				name="sysFtsearchAdvanced"
				path="/sys/relation/sys_relation_search/sysFtsearchAdvanced.jsp">
			</forward>
			<forward
				name="staticPageSearch"
				path="/sys/relation/sys_relation_main/sysRelationStaticPage_searchDialog.jsp">
			</forward>
			<forward
				name="searchResultPage"
				path="/sys/relation/sys_relation_main/sysRelationStaticPage_searchResultPage.jsp">
			</forward>
		</action>
		<action
			path="/sys/relation/relation"
			type="com.landray.kmss.sys.relation.actions.SysRelationAction"
			parameter="method"
			scope="request">
			<forward
				name="relationFtsearch"
				path="/sys/relation/sys_relation_main/sysRelationFtsearch_edit.jsp">
			</forward>
			<forward
				name="relationEntries"
				path="/sys/relation/sys_relation_main/sysRelationModule_select.jsp">
			</forward>
			<forward
				name="relationStaticPage"
				path="/sys/relation/sys_relation_main/sysRelationStaticPage_edit.jsp">
			</forward>
			<forward
				name="relationConditions"
				path="/sys/relation/sys_relation_main/sysRelationCondition_select.jsp">
			</forward>
			<forward
				name="conditionUi"
				path="/sys/relation/import/condition/sysRelationCondition_config.jsp">
			</forward>
			<forward
				name="relationResults"
				path="/sys/relation/sys_relation_main/sysRelationMain_url.jsp">
			</forward>
			<forward
				name="listUi"
				path="/sys/relation/import/sysRelationMain_list.jsp"></forward>
			<forward
				name="moreRelationResults"
				path="/sys/relation/sys_relation_main/sysRelationMain_url_more.jsp">
			</forward>
			<forward
				name="otherMoreResults"
				path="/sys/relation/sys_relation_main/sysRelationMain_otherurl_more.jsp">
			</forward>
			<forward
				name="docResult"
				path="/sys/relation/sys_relation_main/sysRelationMain_doc_result.jsp">
			</forward>
			<forward
				name="docResult.4pda"
				path="/sys/relation/sys_relation_main/sysRelationMain_pda_result.jsp">
			</forward>
			<forward
				name="mobileList"
				path="/sys/relation/mobile/list.jsp" />
		</action>
		<action
			path="/sys/relation/sys_relation_foreign_module/sysRelationForeignModule"
			type="com.landray.kmss.sys.relation.actions.SysRelationForeignModuleAction"
			name="sysRelationForeignModuleForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="view"
				path="/sys/relation/sys_relation_foreign_module/sysRelationForeignModule_view.jsp">
			</forward>
			<forward
				name="edit"
				path="/sys/relation/sys_relation_foreign_module/sysRelationForeignModule_edit.jsp">
			</forward>
			<forward
				name="list"
				path="/sys/relation/sys_relation_foreign_module/sysRelationForeignModule_list.jsp">
			</forward>
		</action>
		<action
			path="/sys/relation/sys_relation_foreign_param/sysRelationForeignParam"
			type="com.landray.kmss.sys.relation.actions.SysRelationForeignParamAction"
			name="sysRelationForeignParamForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="view"
				path="/sys/relation/sys_relation_foreign_param/sysRelationForeignParam_view.jsp">
			</forward>
			<forward
				name="edit"
				path="/sys/relation/sys_relation_foreign_param/sysRelationForeignParam_edit.jsp">
			</forward>
			<forward
				name="list"
				path="/sys/relation/sys_relation_foreign_param/sysRelationForeignParam_list.jsp">
			</forward>
		</action>
		<action
			path="/sys/relation/sys_relation_main/sysRelationFileUpload"
			type="com.landray.kmss.sys.relation.actions.SysRelationFileUploadAction"
			name="sysRelationFileUploadForm"
			parameter="method"
			unknown="false"
			scope="request">
		</action>
		<action
			path="/sys/relation/sys_relation_main/sysRelationOverView"
			type="com.landray.kmss.sys.relation.actions.SysRelationOverViewAction"
			name="sysRelationStaticNewForm"
			parameter="method"
			unknown="false"
			scope="request">
			<forward
				name="listOverView"
				path="/sys/relation/import/sysRelationMain_over_view_list.jsp" />
		</action>
	</action-mappings>
	<message-resources
		parameter="com.landray.kmss.sys.relation.ApplicationResources"
		key="sys-relation" />
</struts-config>

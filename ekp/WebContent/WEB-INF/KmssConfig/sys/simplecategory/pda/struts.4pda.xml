<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<!-- 操作映射 -->
	<action-mappings>
		<action path="/sys/simplecategory/pda/sysSimpleCategory"
			type="com.landray.kmss.sys.simplecategory.pda.PdaSimpleCategoryAction" parameter="method"
			unknown="false" validate="false" scope="request">
			<forward name="pdaSimpleCategory" path="/sys/simplecategory/pda/sysSimpleCategory.jsp"></forward>
		</action>
	</action-mappings>
	<message-resources
		parameter="com.landray.kmss.sys.simplecategory.pda.ApplicationResources"
		key="sys-simplecategory-pda" />
</struts-config>

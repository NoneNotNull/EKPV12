<?xml version="1.0" encoding="UTF-8"?>
<models
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
	<model
		modelName="com.landray.kmss.sys.task.model.SysTaskApprove"
		messageKey="sys-task:table.sysTaskApprove"
		displayProperty="fdApprove"
		serviceBean="sysTaskApproveService"
		table="sys_task_approve">
		<idProperty>
			<generator
				type="assigned" />
		</idProperty>
		<simpleProperty
			name="fdApprove"
			messageKey="sys-task:sysTaskApprove.fdApprove"
			type="String"
			column="fd_approve"
			notNull="true"
			length="200"
			validate="true" />
		<simpleProperty
			name="fdOrder"
			messageKey="sys-task:sysTaskCategory.fdOrder"
			type="Integer"
			column="fd_order"
			notNull="false"
			length="10" />
		<simpleProperty
			name="fdScore"
			messageKey="sys-task:sysTaskApprove.fdScore"
			type="Double"
			column="fd_score"
			notNull="false"
			length="100" />
		<simpleProperty
			name="fdIsAvailable"
			messageKey="sys-task:sysTaskApprove.fdIsAvailable"
			type="Boolean"
			column="fd_is_available"
			notNull="false" />
	</model>
</models>
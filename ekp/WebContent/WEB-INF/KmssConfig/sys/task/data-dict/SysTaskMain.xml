<?xml version="1.0" encoding="UTF-8"?>
<models
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
	<model
		modelName="com.landray.kmss.sys.task.model.SysTaskMain"
		messageKey="sys-task:table.sysTaskMain"
		serviceBean="sysTaskMainService"
		table="sys_task_main"
		displayProperty="docSubject"
		url="/sys/task/sys_task_main/sysTaskMain.do?method=view&amp;fdId=${fdId}">
		<idProperty>
			<generator
				type="assigned" />
		</idProperty>
		<simpleProperty
			name="docSubject"
			messageKey="sys-task:sysTaskMain.docSubject"
			type="String"
			column="doc_subject"
			notNull="true"
			length="200"
			canSearch="true"
			canDisplay="true" 
			validate="true"/>
		<simpleProperty
			name="docCreateTime"
			messageKey="sys-task:sysTaskMain.docCreateTime"
			type="DateTime"
			column="doc_create_time"
			notNull="false"
			canSearch="true"
			canDisplay="true" />
		<simpleProperty
			name="fdKey"
			messageKey="sys-task:sysTaskMain.fdKey"
			type="String"
			column="fd_key"
			notNull="false"
			length="100" 
			canDisplay="false"/>
		<simpleProperty
			name="fdModelName"
			messageKey="sys-task:sysTaskMain.fdModelName"
			type="String"
			column="fd_model_name"
			notNull="false"
			length="100" 
			canDisplay="false"/>
		<simpleProperty
			name="fdModelId"
			messageKey="sys-task:sysTaskMain.fdModelId"
			type="String"
			column="fd_model_id"
			notNull="false"
			length="36" 
			canDisplay="false"/>
		<simpleProperty
			name="fdResolveFlag"
			messageKey="sys-task:sysTaskMain.fdResolveFlag"
			type="String"
			column="fd_resolve_flag"
			notNull="false" 
			canDisplay="false"/>
		<simpleProperty
			name="fdProgressAuto"
			messageKey="sys-task:sysTaskMain.fdProgressAuto"
			type="String"
			column="fd_progress_auto"
			notNull="false" 
			canDisplay="false"/>
		<simpleProperty
			name="fdWeights"
			messageKey="sys-task:sysTaskMain.fdWeights"
			type="Integer"
			column="fd_weights"
			notNull="false"
			length="10"
			validate="true" />
		<simpleProperty
			name="docStatus"
			messageKey="sys-task:sysTaskMain.docStatus"
			type="String"
			column="doc_status"
			notNull="false"
			length="2" 
			canDisplay="false"/>
		<simpleProperty
			name="docReadCount"
			messageKey="sys-task:sysTaskMain.docReadCount"
			type="Long"
			column="doc_read_count"
			notNull="false"
			length="19" 
			canDisplay="true"/>
		<simpleProperty
			name="fdNotifyType"
			messageKey="sys-task:sysTaskMain.fdNotifyType"
			type="String"
			column="fd_notify_type"
			notNull="false"
			length="100" 
			canDisplay="true"
			enumType="sysTaskMain_fdNotifyType"/>
		<simpleProperty
			name="fdSourceSubject"
			messageKey="sys-task:sysTaskMain.fdSourceSubject"
			type="String"
			column="fd_source_subject"
			notNull="false"
			length="200" 
			canDisplay="true"/>
		<simpleProperty
			name="fdSourceUrl"
			messageKey="sys-task:sysTaskMain.fdSourceUrl"
			type="String"
			column="fd_source_url"
			notNull="false"
			length="1000" 
			canDisplay="false"/>
		<simpleProperty
			name="fdPastDue"
			messageKey="sys-task:sysTaskMain.fdPastDue"
			type="String"
			column="fd_past_due"
			notNull="false"
			enumType="common_yesno_number"
			length="1" 
			canDisplay="true"/>
		<simpleProperty
			name="fdHierarchyId"
			messageKey="sys-task:sysTaskMain.fdHierarchyId"
			type="String"
			column="fd_hierarchy_id"
			notNull="false"
			length="1" 
			canDisplay="false"/>
		<simpleProperty
			name="fdPlanCompleteDateTime"
			messageKey="sys-task:sysTaskMain.fdPlanCompleteTime"
			type="DateTime"
			column="fd_plan_complete_date_time"
			notNull="false"
			canSearch="true"
			canDisplay="false" />
		<complexProperty
			name="fdPlanCompleteTime"
			messageKey="sys-task:sysTaskMain.fdPlanCompleteTime"
			type="String"			
			notNull="false"
			canSearch="true"
			canDisplay="true" />
		<complexProperty
			name="fdPlanCompleteDate"
			messageKey="sys-task:sysTaskMain.fdPlanCompleteDate"
			type="String"			
			notNull="false"
			canSearch="true"
			canDisplay="true" />
		<simpleProperty
			name="fdProgress"
			messageKey="sys-task:sysTaskMain.fdProgress"
			type="String"
			column="fd_progress"
			notNull="false"
			canDisplay="true" />
		<simpleProperty
			name="fdStatus"
			messageKey="sys-task:sysTaskMain.fdStatus"
			type="String"
			column="fd_status"
			notNull="false"
			length="2" 
			canDisplay="true"
			enumType="sysTaskMain_fdStatue" />
		<simpleProperty
			name="docContent"
			messageKey="sys-task:sysTaskMain.docContent"
			type="RTF"
			column="doc_content"
			notNull="false"
			canSearch="true"
			canDisplay="true"
			length="1000000" />
		<simpleProperty
			name="fdAttentionCount"
			messageKey="sys-task:sysTaskMain.fdAttentionCount"
			type="Double"
			column="fd_attention_count"
			notNull="false"
			length="10"
			canDisplay="false" />
		<!-- 同步时机 -->
		<simpleProperty
			name="syncDataToCalendarTime"
			messageKey="sys-task:sysTaskMain.syncDataToCalendarTime"
			type="String"
			column="sync_data_to_calendar_time"
			notNull="false"
			unique="false"
			length="500"
			canRelation="false"
			validate="false" 
			canDisplay="false"
			canSearch="false"/>
		<modelProperty
			name="docCreator"
			messageKey="sys-task:sysTaskMain.docCreatorId"
			type="com.landray.kmss.sys.organization.model.SysOrgPerson"
			column="doc_creator_id"
			notNull="false"
			canSearch="true"
			canDisplay="true"
			where="sysOrgElement.fdOrgType=8"
			dialogJS="Dialog_Address(!{mulSelect}, '!{idField}', '!{nameField}', null, ORG_TYPE_PERSON);" />
		<modelProperty
			name="sysTaskFeedback"
			messageKey="sys-task:sysTaskMain.sysTaskFeedback"
			type="com.landray.kmss.sys.task.model.SysTaskFeedback"
			column="fd_feedback_id"
			notNull="false" 
			canDisplay="false"/>
		<modelProperty
			name="sysTaskEvaluate"
			messageKey="sys-task:sysTaskMain.sysTaskEvaluate"
			type="com.landray.kmss.sys.task.model.SysTaskEvaluate"
			column="fd_evaluate_id"
			cascade="all"
			notNull="false" 
			canDisplay="false"/>
		<modelProperty
			name="sysTaskCategory"
			messageKey="sys-task:sysTaskMain.fdCategoryId"
			type="com.landray.kmss.sys.task.model.SysTaskCategory"
			column="fd_category_id"
			notNull="false" 
			canDisplay="true"/>
		<modelProperty
			name="fdAppoint"
			messageKey="sys-task:sysTaskMain.fdAppoint"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			column="fd_appoint"
			notNull="false"
			canSearch="true"
			where="sysOrgElement.fdOrgType=8"
			dialogJS="Dialog_Address(!{mulSelect}, '!{idField}', '!{nameField}', null, ORG_TYPE_PERSON);" 
			canDisplay="true"/>
		<modelProperty
			name="rootSysTaskMain"
			messageKey="sys-task:sysTaskMain.fdRootId"
			type="com.landray.kmss.sys.task.model.SysTaskMain"
			column="fd_root_id"
			notNull="false" 
			canDisplay="false"/>
		<listProperty
			name="authReaders"
			messageKey="sys-task:sysTaskMainReader.authReader"
			column="fd_task_main_id"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			elementColumn="auth_reader_id"
			table="sys_task_main_reader" 
			canDisplay="true"/>
		<listProperty
			name="authAllReaders"
			messageKey="sys-task:sysTaskMainReader.authAllReaders"
			column="fd_task_main_id"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			elementColumn="auth_all_reader_id"
			table="sys_task_main_areaders"
			canDisplay="false" />
		<listProperty
			name="toSysOrgPerform"
			messageKey="sys-task:sysTaskMainPerform.fdPerformId"
			column="fd_task_id"
			notNull="true"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			elementColumn="fd_perform_id"
			table="sys_task_main_perform"
			canSearch="true"
			dialogJS="Dialog_Address(!{mulSelect}, '!{idField}', '!{nameField}', null, ORG_TYPE_PERSON);" />
		<listProperty
			name="toSysOrgCc"
			messageKey="sys-task:sysTaskMainPerform.toSysOrgCc"
			column="fd_task_id"
			table="sys_task_main_cc"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			elementColumn="fd_cc_id" />
		<listProperty
			name="toSysOrgAttention"
			messageKey="sys-task:sysTaskMainPerform.toSysOrgAttention"
			column="fd_task_id"
			table="sys_task_attention"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			elementColumn="fd_attention_id" 
			canDisplay="false"/>
		<listProperty
			name="authEditors"
			messageKey="sys-task:sysTaskMainPerform.authEditors"
			column="fd_task_main_id"
			table="sys_task_main_editor"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			elementColumn="auth_editor_id" 
			canDisplay="false"/>
		<listProperty
			name="authAllEditors"
			messageKey="sys-task:sysTaskMainPerform.authAllEditors"
			column="fd_task_main_id"
			table="sys_task_main_aeditor"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			elementColumn="auth_all_editor_id"
			canDisplay="false" />
		<listProperty
			name="authOtherEditors"
			messageKey="sys-task:sysTaskMainPerform.authOtherEditors"
			column="fd_task_main_id"
			table="sys_task_main_oeditor"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			elementColumn="auth_other_editor_id" 
			canDisplay="false"/>
		<listProperty
			name="authOtherReaders"
			messageKey="sys-task:sysTaskMainPerform.authOtherReaders"
			column="fd_task_main_id"
			table="sys_task_main_oreader"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			elementColumn="auth_other_reader_id" 
			canDisplay="false"/>
		<listProperty
			name="rootChildList"
			messageKey="sys-task:sysTaskMainPerform.rootChildList"
			column="fd_root_id"
			type="com.landray.kmss.sys.task.model.SysTaskMain" 
			canDisplay="false"/>
		<modelProperty
			name="sysTaskMainParent"
			messageKey="sys-task:sysTaskMain.fdParentId"
			type="com.landray.kmss.sys.task.model.SysTaskMain"
			column="fd_parent_id" />
		<attachmentProperty
			name="attachment"
			messageKey="sys-task:sysTaskMain.attachment" />

	</model>
</models>
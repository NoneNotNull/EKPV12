	length="200"
/>
<simpleProperty
	name="fdNo"
	messageKey="sys-notify:sysNotifyCategory.fdNo"
	type="String"
	column="fd_no"
	notNull="false"
	length="50"
/>
<simpleProperty
	name="fdOrder"

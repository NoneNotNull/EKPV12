<?xml version="1.0" encoding="UTF-8"?>
<models xmlns="http://www.example.org/design-config" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
<model
	modelName="com.landray.kmss.sys.notify.model.SysNotifyCategory"
	messageKey="sys-notify:table.sysNotifyCategory "
	displayProperty="fdName"
	serviceBean="sysNotifyCategoryService"
	table="sys_notify_category"

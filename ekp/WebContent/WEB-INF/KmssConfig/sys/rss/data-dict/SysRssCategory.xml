<?xml version="1.0" encoding="UTF-8"?>
<models xmlns="http://www.example.org/design-config" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
<model
	modelName="com.landray.kmss.sys.rss.model.SysRssCategory"
	messageKey="sys-rss:table.sysRssCategory"
	displayProperty="fdName"
	serviceBean="sysRssCategoryService"
	table="sys_rss_category"
>
<idProperty><generator
	type="assigned"
/></idProperty>
<simpleProperty
	name="fdName"
	messageKey="sys-rss:sysRssCategory.fdName"
	type="String"
	column="fd_name"
	notNull="false"
	length="200"
/>
<simpleProperty
	name="docCreateTime"
	messageKey="sys-rss:sysRssCategory.docCreateTime"
	type="DateTime"
	column="doc_create_time"
	notNull="false"
/>
<simpleProperty
	name="docAlterTime"
	messageKey="sys-rss:sysRssCategory.docAlterTime"
	type="DateTime"
	column="doc_alter_time"
	notNull="false"
/>
<simpleProperty
	name="fdOrder"
	messageKey="sys-rss:sysRssCategory.fdOrder"
	type="Integer"
	column="fd_order"
	notNull="false"
	length="10"
/>
<modelProperty
	name="docCreator"
	messageKey="sys-rss:sysRssCategory.docCreator"
	type="com.landray.kmss.sys.organization.model.SysOrgElement"
	column="doc_creator_id"
	notNull="false"
/>
<modelProperty
	name="docAlteror"
	messageKey="sys-rss:sysRssCategory.docAlteror"
	type="com.landray.kmss.sys.organization.model.SysOrgElement"
	column="doc_alteror_id"
	notNull="false"
/>
<modelProperty
	name="fdParent"
	messageKey="sys-rss:sysRssCategory.fdParent"
	type="com.landray.kmss.sys.rss.model.SysRssCategory"
	column="fd_parent_id"
	cascade="save-update"
/>
</model></models>
<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<!-- 表单 Bean -->
	<form-beans>
		<!-- 常用分类 -->
		<form-bean
			name="sysPersonFavoriteCategoryForm"
			type="com.landray.kmss.sys.person.forms.SysPersonFavoriteCategoryForm">
		</form-bean>
		<form-bean
			name="sysPersonFavoriteCategoryAllForm"
			type="com.landray.kmss.sys.person.forms.SysPersonFavoriteCategoryAllForm">
		</form-bean>
		<!-- 个人导航 -->
		<form-bean
			name="sysPersonSysNavCategoryForm"
			type="com.landray.kmss.sys.person.forms.SysPersonSysNavCategoryForm">
		</form-bean>
		<form-bean
			name="sysPersonSysNavLinkForm"
			type="com.landray.kmss.sys.person.forms.SysPersonSysNavLinkForm">
		</form-bean>
		<form-bean
			name="sysPersonMyNavCategoryForm"
			type="com.landray.kmss.sys.person.forms.SysPersonMyNavCategoryForm">
		</form-bean>
		<form-bean
			name="sysPersonMyNavLinkForm"
			type="com.landray.kmss.sys.person.forms.SysPersonMyNavLinkForm">
		</form-bean>
		<!-- 横向导航 -->
		<form-bean
			name="sysPersonSysTabCategoryForm"
			type="com.landray.kmss.sys.person.forms.SysPersonSysTabCategoryForm">
		</form-bean>
		<form-bean
			name="sysPersonSysTabLinkForm"
			type="com.landray.kmss.sys.person.forms.SysTabsonSysTabLinkForm">
		</form-bean>
		<form-bean
			name="sysPersonMyTabCategoryForm"
			type="com.landray.kmss.sys.person.forms.SysPersonMyTabCategoryForm">
		</form-bean>
		<form-bean
			name="sysPersonMyTabLinkForm"
			type="com.landray.kmss.sys.person.forms.SysPersonMyTabLinkForm">
		</form-bean>
		<!-- 空间导航 -->
		<form-bean
			name="sysPersonZoneNavigationForm"
			type="com.landray.kmss.sys.person.forms.SysPersonZoneNavigationForm">
		</form-bean>
		<form-bean
			name="sysPersonCfgLinkForm"
			type="com.landray.kmss.sys.person.forms.SysPersonCfgLinkForm">
		</form-bean>
		<form-bean
			name="sysPersonCfgLinkAllForm"
			type="com.landray.kmss.sys.person.forms.SysPersonCfgLinkAllForm">
		</form-bean>
	</form-beans>
	
	<!-- 操作映射 -->
	<action-mappings>
		<action
			path="/sys/person/setting"
			type="com.landray.kmss.sys.person.actions.SysPersonSettingAction"
			validate="false"
			scope="request">
			<forward
				name="index"
				path="/sys/person/index.jsp">
			</forward>
		</action>
		<action
			path="/sys/person/home"
			type="com.landray.kmss.sys.person.actions.SysPersonHomeAction"
			validate="false"
			scope="request">
			<forward
				name="index"
				path="/sys/person/home.jsp">
			</forward>
		</action>
		<action
			path="/sys/person/zone"
			type="com.landray.kmss.sys.person.actions.SysPersonZoneAction"
			parameter="method"
			validate="false"
			scope="request">
			<forward
				name="zone"
				path="/sys/person/zone.jsp">
			</forward>
		</action>
		<!-- 常用分类 -->
		<action
			path="/sys/person/sys_person_favorite_category/sysPersonFavoriteCategory"
			type="com.landray.kmss.sys.person.actions.SysPersonFavoriteCategoryAction"
			name="sysPersonFavoriteCategoryAllForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="edit"
				path="/sys/person/sys_person_favorite_category/sysPersonFavoriteCategory_edit.jsp">
			</forward>
			<forward
				name="view"
				path="/sys/person/sys_person_favorite_category/sysPersonFavoriteCategory_edit.jsp">
			</forward>
		</action>
		<!-- 个人导航 -->
		<action
			path="/sys/person/sys_person_mynav_category/sysPersonMyNavCategory"
			type="com.landray.kmss.sys.person.actions.SysPersonMyNavCategoryAction"
			name="sysPersonMyNavCategoryForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="list"
				path="/sys/person/sys_person_mynav_category/sysPersonMyNavCategory_list.jsp">
			</forward>
			<forward
				name="edit"
				path="/sys/person/sys_person_mynav_category/sysPersonMyNavCategory_edit.jsp">
			</forward>
			<forward
				name="view"
				path="/sys/person/sys_person_mynav_category/sysPersonMyNavCategory_edit.jsp">
			</forward>
			<forward
				name="portlet"
				path="/sys/person/sys_person_mynav_category/sysPersonMyNavCategory_portlet.jsp">
			</forward>
			<forward
				name="select"
				path="/sys/person/sys_person_link/sysPersonLink_select.jsp">
			</forward>
			<forward
				name="dialog"
				path="/sys/person/sys_person_link/sysPersonLink_dialog.jsp">
			</forward>
			<forward
				name="fragment"
				path="/sys/person/sys_person_mynav_category/sysPersonMyNavCategory_listFragment.jsp">
			</forward>
		</action>
		<action
			path="/sys/person/sys_person_sysnav_category/sysPersonSysNavCategory"
			type="com.landray.kmss.sys.person.actions.SysPersonSysNavCategoryAction"
			name="sysPersonSysNavCategoryForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="list"
				path="/sys/person/sys_person_sysnav_category/sysPersonSysNavCategory_list.jsp">
			</forward>
			<forward
				name="edit"
				path="/sys/person/sys_person_sysnav_category/sysPersonSysNavCategory_edit.jsp">
			</forward>
			<forward
				name="view"
				path="/sys/person/sys_person_sysnav_category/sysPersonSysNavCategory_edit.jsp">
			</forward>
		</action>
		<!-- 横向导航 -->
		<action
			path="/sys/person/sys_person_mytab_category/sysPersonMyTabCategory"
			type="com.landray.kmss.sys.person.actions.SysPersonMyTabCategoryAction"
			name="sysPersonMyTabCategoryForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="list"
				path="/sys/person/sys_person_mytab_category/sysPersonMyTabCategory_list.jsp">
			</forward>
			<forward
				name="all"
				path="/sys/person/sys_person_mytab_category/sysPersonMyTabCategory_all.jsp">
			</forward>
			<forward
				name="edit"
				path="/sys/person/sys_person_mytab_category/sysPersonMyTabCategory_edit.jsp">
			</forward>
			<forward
				name="view"
				path="/sys/person/sys_person_mytab_category/sysPersonMyTabCategory_edit.jsp">
			</forward>
			<forward
				name="portlet"
				path="/sys/person/sys_person_mytab_category/sysPersonMyTabCategory_portlet.jsp">
			</forward>
			<forward
				name="select"
				path="/sys/person/sys_person_link/sysPersonLink_select.jsp">
			</forward>
			<forward
				name="dialog"
				path="/sys/person/sys_person_link/sysPersonLink_dialog.jsp">
			</forward>
			<forward
				name="fragment"
				path="/sys/person/sys_person_mytab_category/sysPersonMyTabCategory_listFragment.jsp">
			</forward>
		</action>
		<action
			path="/sys/person/sys_person_systab_category/sysPersonSysTabCategory"
			type="com.landray.kmss.sys.person.actions.SysPersonSysTabCategoryAction"
			name="sysPersonSysTabCategoryForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="list"
				path="/sys/person/sys_person_systab_category/sysPersonSysTabCategory_list.jsp">
			</forward>
			<forward
				name="all"
				path="/sys/person/sys_person_systab_category/sysPersonSysTabCategory_all.jsp">
			</forward>
			<forward
				name="page"
				path="/sys/person/sys_person_systab_category/sysPersonSysTabCategory_page.jsp">
			</forward>
			<forward
				name="edit"
				path="/sys/person/sys_person_systab_category/sysPersonSysTabCategory_edit.jsp">
			</forward>
			<forward
				name="view"
				path="/sys/person/sys_person_systab_category/sysPersonSysTabCategory_edit.jsp">
			</forward>
		</action>
		<!-- 个人空间 -->
		<action
			path="/sys/person/sys_person_zone/sysPersonZone"
			type="com.landray.kmss.sys.person.actions.SysPersonZoneAction"
			parameter="method"
			input="view"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="view"
				path="/sys/person/zone.jsp">
			</forward>
		</action>
		<!-- 系统链接选择 -->
		<action
			path="/sys/person/sys_person_link/sysPersonLink"
			type="com.landray.kmss.sys.person.actions.SysPersonLinkAction"
			parameter="method"
			input="view"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="select"
				path="/sys/person/sys_person_link/sysPersonLink_select.jsp">
			</forward>
			<forward
				name="dialog"
				path="/sys/person/sys_person_link/sysPersonLink_dialog.jsp">
			</forward>
			<forward
				name="search"
				path="/sys/person/sys_person_link/sysPersonLink_search.jsp">
			</forward>
		</action>
		<!-- 个人设置 -->
		<action
			path="/sys/person/sys_person_cfg_link/sysPersonCfgLink"
			type="com.landray.kmss.sys.person.actions.SysPersonCfgLinkAction"
			name="sysPersonCfgLinkAllForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="edit"
				path="/sys/person/sys_person_cfg_link/sysPersonCfgLink_edit.jsp">
			</forward>
			<forward
				name="select"
				path="/sys/person/sys_person_cfg_link/sysPersonCfgLink_select.jsp">
			</forward>
		</action>
	</action-mappings>
	
	<message-resources
		parameter="com.landray.kmss.sys.person.ApplicationResources"
		key="sys-person" />
</struts-config>

<?xml version="1.0" encoding="UTF-8"?>
<models
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
	<model
		modelName="com.landray.kmss.kms.integral.model.KmsIntegralType"
		messageKey="kms-integral:table.kmsIntegralType"
		displayProperty="fdName"
		serviceBean="kmsIntegralTypeService"
		table="kms_integral_type"
		url="/kms/integral/kms_integral_type/kmsIntegralType.do?method=view&amp;fdId=${fdId}">

		<idProperty>
			<generator
				type="assigned" />
		</idProperty>

		<simpleProperty
			name="fdName"
			messageKey="kms-integral:kmsIntegralType.fdName"
			type="String"
			column="fd_name"
			notNull="true"
			length="200"
			validate="true" />

		<simpleProperty
			name="fdOrder"
			messageKey="kms-integral:kmsIntegralType.fdOrder"
			type="Integer"
			column="fd_order"
			notNull="false"
			length="10"
			validate="true" />

	</model>
</models>

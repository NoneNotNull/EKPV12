			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="view"
				path="/kms/ask/kms_ask_agree/kmsAskAgree_view.jsp" />
			<forward
				name="edit"
				path="/kms/ask/kms_ask_agree/kmsAskAgree_edit.jsp" />
			<forward
				name="list"
				path="/kms/ask/kms_ask_agree/kmsAskAgree_list.jsp" />
		</action>

		<action
			path="/kms/ask/kms_ask_comment/kmsAskComment"

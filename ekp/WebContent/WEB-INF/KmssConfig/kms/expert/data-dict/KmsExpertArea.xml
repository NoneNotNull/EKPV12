<?xml version="1.0" encoding="UTF-8"?>
<models
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
	<model
		modelName="com.landray.kmss.kms.expert.model.KmsExpertArea"
		messageKey="kms-expert:table.kmsExpertArea"
		displayProperty="fdExpertId"
		serviceBean="kmsExpertAreaService"
		table="kms_expert_area"
		url="/kms/expert/kms_expert_area/kmsExpertArea.do?method=view&amp;fdId=${fdId}">

		<idProperty>
			<generator
				type="assigned" />
		</idProperty>

		<simpleProperty
			name="fdExpertId"
			messageKey="kms-expert:kmsExpertArea.fdExpertId"
			type="String"
			column="fd_expert_id"
			notNull="true"
			length="200"
			validate="false" />

		<simpleProperty
			name="fdCategoryId"
			messageKey="kms-expert:kmsExpertArea.fdCategoryId"
			type="String"
			column="fd_category_id"
			notNull="true"
			length="200"
			validate="false" />

		<simpleProperty
			name="docCreateTime"
			messageKey="kms-expert:kmsExpertArea.docCreateTime"
			type="DateTime"
			column="doc_create_time"
			notNull="false"
			validate="false" />

		<simpleProperty
			name="fdModelName"
			messageKey="kms-expert:kmsExpertArea.fdModel"
			type="String"
			column="fd_model_name"
			notNull="true"
			length="200"
			validate="false" />
			
		<simpleProperty
			name="fdKey"
			messageKey="kms-expert:kmsExpertArea.fdKey"
			type="String"
			column="fd_key"
			notNull="false"
			length="200"
			validate="false" />
			
	</model>
</models>

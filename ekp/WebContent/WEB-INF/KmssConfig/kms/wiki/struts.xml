				name="tree"
				path="/kms/wiki/kms_wiki_category/kmsWikiCategory_tree.jsp">
			</forward>
			<forward
				name="move"
				path="/kms/wiki/kms_wiki_category/kmsWikiCategory_move.jsp">
			</forward>
			<forward
				name="listTemplate"
				path="/kms/wiki/pda/module_tempdata.jsp">
			</forward>
		</action>
		

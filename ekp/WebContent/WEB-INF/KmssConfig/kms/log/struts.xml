		<form-bean
			name="kmsLogContextModuleForm"
			type="com.landray.kmss.kms.log.forms.KmsLogContextModuleForm" />
		<form-bean
			name="kmsLogDocContextForm"
			type="com.landray.kmss.kms.log.forms.KmsLogDocContextForm" />

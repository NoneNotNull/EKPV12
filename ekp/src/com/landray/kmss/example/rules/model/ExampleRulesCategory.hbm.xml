<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.example.rules.model.ExampleRulesCategory"
		table="example_rules_category">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="hbmParent"
			class="com.landray.kmss.example.rules.model.ExampleRulesCategory"
			column="fd_parent_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />


       <!-- 修改人 -->
		<many-to-one
			name="docAlteror"
			column="doc_alteror_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<!-- 修改时间 -->
		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<!-- 默认可阅读者 -->
		<bag
			name="authTmpReaders"
			table="example_rules_cate_def_reader"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>

		<!-- 默认可编辑者 -->
		<bag
			name="authTmpEditors"
			table="example_rules_cate_def_editor"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		
		<!-- 所有人不可使用 -->
		<property
			name="authNotReaderFlag"
			column="auth_not_reader_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
			
		<!-- 所有可使用 -->
		<property
			name="authReaderFlag"
			column="auth_reader_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
			
		<!-- 可使用者 -->
		<bag
			name="authReaders"
			table="example_rules_cate_reader"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		
		<!-- 其他可使用者 -->
		<bag
			name="authOtherReaders"
			table="example_rules_cate_oreader"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		
		<!-- 所有可使用者 -->
		<bag
			name="authAllReaders"
			table="example_rules_cate_areader"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_reader_id" />
		</bag>
		
		<!-- 可编辑者 -->
		<bag
			name="authEditors"
			table="example_rules_cate_editor"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		
		<!-- 其他可编辑者 -->
		<bag
			name="authOtherEditors"
			table="example_rules_cate_oeditor"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>

		<!-- 所有可编辑者 -->
		<bag
			name="authAllEditors"
			table="example_rules_cate_aeditor"
			lazy="true">
			<key
				column="fd_tmp_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_editor_id" />
		</bag>
	</class>
</hibernate-mapping>

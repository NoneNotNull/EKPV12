<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.tib.sys.sap.connector.model.TibSysSapRfcCategory"
		table="tib_sys_sap_rfc_category">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<property
			name="fdHierarchyId"
			column="fd_hierarchy_id"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />

	<many-to-one
			name="hbmParent"
			class="com.landray.kmss.tib.sys.sap.connector.model.TibSysSapRfcCategory"
			cascade="save-update"
			outer-join="auto"
			update="true"
			insert="true"
			access="property"
			column="fd_parent_id" /> 
			
		<bag
			name="authReaders"
			table="tib_sap_rfc_category_reader"
			lazy="true">
			<key
				column="doc_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		<bag
			name="authEditors"
			table="tib_sap_rfc_category_editor"
			lazy="true">
			<key
				column="doc_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
		<many-to-one
			name="docAlteror"
			column="doc_alteror_id"
			insert="true"
			update="true"
			not-null="false" />
		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />
		<bag
			name="authAllEditors"
			table="tib_sys_sap_category_aeditor"
			lazy="true">
			<key
				column="doc_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		<bag
			name="authAllReaders"
			table="tib_sys_sap_category_areader"
			lazy="true">
			<key
				column="doc_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		<bag
			name="authOtherReaders"
			table="tib_sys_sap_category_oreader"
			lazy="true">
			<key
				column="doc_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		<bag
			name="authOtherEditors"
			table="tib_sys_sap_category_oeditor"
			lazy="true">
			<key
				column="doc_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>

		<property
			name="authNotReaderFlag"
			column="auth_not_reader_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<bag
			name="authTmpReaders"
			table="tib_sys_sap_tmp_main_reader"
			lazy="true">
			<key
				column="doc_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		<bag
			name="authTmpEditors"
			table="tib_sys_sap_tmp_main_editor"
			lazy="true">
			<key
				column="doc_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		<property
			name="authReaderFlag"
			column="auth_reader_flag"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="authEditorFlag"
			column="auth_editor_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />

	</class>
</hibernate-mapping>

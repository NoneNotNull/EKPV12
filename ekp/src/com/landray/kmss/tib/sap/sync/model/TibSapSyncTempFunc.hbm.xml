<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.tib.sap.sync.model.TibSapSyncTempFunc"
		table="tib_sap_sync_temp_func">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdInvokeType"
			column="fd_invoke_type"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdFuncMark"
			column="fd_func_mark"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />

		<property
			name="fdRfcXml"
			column="fd__rfc_Xml"
			update="true"
			insert="true"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true"
			length="100000"
			not-null="false" />
		<property
			name="fdUse"
			column="fd_use"
			update="true"
			insert="true"
			not-null="false" />
			
				<property
			name="fdSendType"
			column="fd_send_type"
			update="true"
			insert="true"
			not-null="false" />


		<property
			name="fdEditorTime"
			column="fd_editor_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdQuartzTime"
			column="fd_quartz_time"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="fdRfcSetting"
			column="fd_rfc_setting_id"
			insert="true"
			update="true"
			not-null="true" />

		<many-to-one
			name="fdQuartz"
			column="fd_quartz_id"
			insert="true"
			update="true"
			not-null="false" />

	</class>
</hibernate-mapping>

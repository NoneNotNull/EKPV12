<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.kms.knowledge.model.KmsKnowledgeWikiCatalog"
		table="kms_knowledge_wiki_catalog">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="docContent"
			column="doc_content"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

		<property
			name="fdKey"
			column="fd_key"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<many-to-one
			name="fdTemplate"
			column="fd_template_id"
			insert="true"
			update="true"
			not-null="false" />

		<bag
			name="authTmpEditors"
			table="kms_knowledge_catalog_teditor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>

	</class>
</hibernate-mapping>
